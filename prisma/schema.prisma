generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Request {
  id          String  @id @default(uuid())
  category    String
  budget      String
  pages       String
  delay       String
  name        String
  email       String
  company     String?
  website     String?
  projectDescription String
  createdAt   DateTime @default(now())
}
